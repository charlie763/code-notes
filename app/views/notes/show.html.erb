<h1>
  <%= @note.title %>
  <%= render partial: "display_tags", locals: {note: @note} %>
</h1>
<span><%= link_to "Edit", edit_note_path(@note) %></span>
<p><%= @note.summary %></p>
<% @note.code_snippets.each do |snippet| %>
  <p><%= snippet.code %></p>
  <p><%= snippet.annotation %></p>
<% end %>

